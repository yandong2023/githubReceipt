{
  "manifest_version": 3,
  "name": "Rednote Translator",
  "version": "1.0.0",
  "description": "A professional translation plugin for Rednote",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/content.js"],
      "run_at": "document_start"
    }
  ]
} 